record(calcout, "ML:DIAG0:AUTO:MASK") {
    field(DESC, "description")
    field(CALC, "A=1&B=1?3:B?2:A?1:0")
    field(INPA, "ML:DIAG0:AUTO:PROG_CTRL")
    field(INPB, "ML:DIAG0:AUTO:PROG_CTRL2")
    field(OUT,  "ML:DIAG0:AUTO:PROG_SELECT.SELN")
    field(OOPT, "Every Time")
}

record(seq, "ML:DIAG0:AUTO:PROG") {
    field(SCAN, "400 second")
    field(DESC, "description")
    field(SELM, "Mask")
    field(SELN, "1")
    field(DOL1, "1")
    field(LNK1, "ML:DIAG0:AUTO:PROGRAM.PROC")
    field(DLY2, "200")
    field(DOL2, "1")
    field(LNK2, "ML:DIAG0:AUTO:PROGRAM2.PROC")
}



